{
 "argv": [
  "python",
  "-m",
  "ipykernel_launcher",
  "-f",
  "{connection_file}"
 ],
 "display_name": "Sample Kernel",
 "language": "python",
 "metadata": {
  "debugger": true,
  "kernel_gateway": {
    "notebook_http_mode": true,
    "default_env": {
      "CUSTOM_KERNEL": "1",
      "NOTEBOOK_PATH": "examples/sample_notebook.ipynb"
    }
  }
 }
} 